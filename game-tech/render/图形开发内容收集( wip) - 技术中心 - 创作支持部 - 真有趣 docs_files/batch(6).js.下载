Confluence.Templates.Editor.Page.quitDialogHeader = function(opt_data, opt_ignored) {
  var output = '';
  switch (opt_data.dialogType) {
    case 'delete':
    case 'revert':
      output += (opt_data.newPage) ? (opt_data.contentType == 'blogpost') ? '<h2 class="aui-dialog2-header-main">' + soy.$$escapeHtml('\u5220\u9664\u672a\u53d1\u5e03\u7684\u535a\u5ba2\u5e16\uff1f') + '</h2>' : '<h2 class="aui-dialog2-header-main">' + soy.$$escapeHtml('\u5220\u9664\u672a\u53d1\u5e03\u7684\u9875\u9762\uff1f') + '</h2>' : '<h2 class="aui-dialog2-header-main">' + soy.$$escapeHtml('\u6062\u590d\u5230\u6700\u65b0\u5df2\u53d1\u5e03\u7248\u672c\uff1f') + '</h2>';
      break;
    case 'publish':
    case 'update':
      output += '<h2 class="aui-dialog2-header-main">' + soy.$$escapeHtml('\u53d1\u5e03\u6240\u6709\u53d8\u66f4\uff1f') + '</h2>';
      break;
    case 'diff':
      output += '<h2 class="aui-dialog2-header-main">' + soy.$$escapeHtml('\u672a\u53d1\u5e03\u7684\u53d8\u66f4') + '</h2>';
      break;
  }
  output += '<a class="aui-dialog2-header-close"><span class="aui-icon aui-icon-small aui-iconfont-close-dialog">' + soy.$$escapeHtml('\u53d6\u6d88') + '</span></a>';
  return output;
};
